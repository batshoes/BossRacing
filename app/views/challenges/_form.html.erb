<%= form_with(model: challenge, local: true) do |form| %>

  <div class="field">
    <%= form.hidden_field :challenger_id, value: current_user.id %>
    <%= form.label :challengee %>
    <br>
    <%= form.select :challengee_id, options_from_collection_for_select(@challengers, :id, :username) %>
  </div>
<!--
  <div class="field">
    <%= form.label :start_time %>

    <%= form.datetime_select :start_time %>
  </div> -->

  <div class="actions">
    <%= form.submit "Create Challenge" %>
  </div>
<% end %>
